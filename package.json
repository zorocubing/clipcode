{
  "name": "clipcode",
  "displayName": "clipcode",
  "description": "Clip Code is a VS Code extension pulls your local Ollama models and use them to copilot your coding session without leaving the editor.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "clipcode",
          "title": "Clip Code",
          "icon": "clipcode.svg"
        }
      ]
    },
    "views": {
      "clipcode": [
        {
          "type": "webview",
          "icon": "clipcode.svg",
          "id": "clipcodeView",
          "name": ""
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.4.0"
  }
}
